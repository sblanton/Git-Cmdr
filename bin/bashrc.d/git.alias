#-- Git Aliases
alias gs='git status -sb'
alias gdf='git diff'

alias gco='git checkout'
alias gcom='git checkout master'

alias gb='git branch'
alias gbt='git branch --track'
alias gsu='git branch --set-upstream'
alias gr='git remote'

alias ga='git add'
alias gc='git commit'
alias grm='git rm'

alias gm='git merge'
alias gpl='git pull'
alias gps='git push'

alias gcfg='git config --global'
alias gcfl='git config --local'

alias blame='git log --name-status HEAD^..HEAD'
